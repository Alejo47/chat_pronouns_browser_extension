{
	"name": "Twitch Chat pronouns",
	"version": "0.1.0",
	"description": "Add Pronouns to Twitch Chat",
	"content_scripts": [
		{
			"matches": [
				"https://www.twitch.tv/*",
				"https://twitch.tv/*",
				"https://dashboard.twitch.tv/u/*/stream-manager"
			],
			"js": [
				"dist/bundle.js"
			],
			"run_at": "document_end",
			"all_frames": true
		}
	],
	"manifest_version": 2
}
